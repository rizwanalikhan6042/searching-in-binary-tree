// Complete the searchInBST function below.
/* For your reference:
struct node
{
    long long value;
    node *left;
    node *right;
};
*/

struct node* searchInBST(node* t,int key)
{
    //write your code here
    if(t==NULL)
    return NULL;
    if(key>t->value)
    return searchInBST(t->right,key);
    else if(key<t->value)
    return searchInBST(t->left,key);
    else
    return t;
}
